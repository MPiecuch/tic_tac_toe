config_version=5

[application]

config/name="tic_tac_toe"
run/main_scene="res://resource/scn/start.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
config/icon="res://resource/img/icon.svg"

[autoload]

GlobalVars="*res://source/GlobalVars.cs"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="tic_tac_toe"

[rendering]

textures/canvas_textures/default_texture_filter=0
environment/defaults/default_clear_color=Color(0.333333, 0.333333, 0.333333, 1)
anti_aliasing/quality/msaa_2d=2
